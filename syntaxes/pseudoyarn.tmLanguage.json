{
  "fileTypes": ["sy"],
  "name": "Pseudoyarn",
  "patterns": [
    {
      "name": "comment.line",
      "match": "#.*"
    },
    {
      "name": "string.quoted.double",
      "begin": "\"",
      "end": "\""
    },
    {
      "name": "string.quoted.single",
      "begin": "'",
      "end": "'"
    },
    {
      "name": "keyword.control",
      "match": "\\b(if|for|while|from|to|is|is not|ERROR|and|or|not|in|out|return|break|continue|function|class|method|property|constructor|super|self|this|true|false|null|undefined|void|int|float|string|bool|array|object|any|root|else|else if|elif|elif not|import|from|as|try|catch|finally|raise|throw|assert|pass|global|print|input|open|close|read|write|append|debug|private|public|protected|static|constant|abstract|override|virtual|inherits|interface)\\b"
    },
    {
      "name": "entity.name.function",
      "match": "\\b\\w+\\s*\\([^\\)]*\\)"
    },
    {
      "name": "variable.assignment",
      "match": "\\b\\w+\\s*(<-|=)\\s*\\w+\\b"
    }
  ],
  "scopeName": "source.pseudoyarn"
}
